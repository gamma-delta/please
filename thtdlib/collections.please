(defun map/keys (map) (for car (map->list map)))
(defun map/vals (map) (for cdr (map->list map)))

(defun index1 (l x) 
  (which l
    [pair? (list/nth l x)]
    [map? (map/get l x)]
    (! 'index1/bad-type 
      (\ (x) (string "cannot index " (car x) " with " (second x))) 
      (list (typeof l) x))))
