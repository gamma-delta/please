; Test long recursions
(defun ackermann (m n) (match (m n)
  [(0 n) (+ n 1)]
  [(m 0) (ackermann (-- m) 1)]
  [(m n) (ackermann (-- m) (ackermann m (-- n)))]))

(map (\ ((m n expect))
  (assert-eq (ackermann m n) expect))
  ; https://en.wikipedia.org/wiki/Ackermann_function#Table_of_values
  '(
    (0 0 1)
    (0 1 2)
    (0 2 3)
    (0 3 4)
    (1 0 2)
    (1 1 3)
    (1 2 4)
    (1 3 5)
    (3 0 5)
    (3 1 13)
    (3 2 29)
    (3 3 61)
    (3 4 125)
    (4 0 13)
    (4 1 65533)))
